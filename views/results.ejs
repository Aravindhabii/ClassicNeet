<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<link rel="shortcut icon" href="images/home/classic-nee-tacademy-logo.png" type="image/x-icon" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<link rel="stylesheet" href="stylesheets/results.css" />
		<link
			rel="stylesheet"
			href="https://unpkg.com/swiper@7/swiper-bundle.min.css"
		/>
		<link rel="stylesheet" href="stylesheets/chatbot.css" />
		<link rel="stylesheet" href="stylesheets/pagination.css" />
		<link rel="shortcut icon" href="../../images/home/classic-nee-tacademy-logo.png" type="image/x-icon" />

		<title>Results</title>
		<style>
			.resultsnav {
				background-color: #369afe;
				border-radius: 0% 50% 0% 50%;
				color: white !important;
			}
		</style>
	</head>
	<body>
		<%- include('./layout/header') %> <%- include('./chatbot') %>
		<section class="swiper">
			<!-- Additional required wrapper -->
			<div class="swiper-wrapper">
				<!-- Slides -->
				<% slider.forEach(i => { %>
				<div class="swiper-slide">
					<img src="<%= i.sliderimg %>" alt="" />
				</div>
				<% }) %>
			</div>
			<!-- If we need pagination -->
			<div class="swiper-pagination"></div>

			<!-- If we need navigation buttons -->
			<div class="swiper-button-prev"></div>
			<div class="swiper-button-next"></div>

			<!-- If we need scrollbar -->
			<div class="swiper-scrollbar"></div>
		</section>

		<section class="section2">
			<select
				class="form-select form-select-lg mb-3 dropdown"
				aria-label=".form-select-lg example"
				data-select=""
			>
				<% year.reverse().forEach((e,i) => { %>
				<option value="<%= e %>"><%= e %></option>
				<% }) %>
			</select>
			<div class="gridcontainer"></div>
			<div id="pagination-container"></div>
		</section>

		<%- include('./layout/footer') %>

		<link
			rel="stylesheet"
			href="https://unpkg.com/swiper@7/swiper-bundle.min.css"
		/>
		<script
			src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"
			referrerpolicy="no-referrer"
		></script>
		<script
			src="https://cdnjs.cloudflare.com/ajax/libs/paginationjs/2.1.5/pagination.min.js"
			integrity="sha512-1zzZ0ynR2KXnFskJ1C2s+7TIEewmkB2y+5o/+ahF7mwNj9n3PnzARpqalvtjSbUETwx6yuxP5AJXZCpnjEJkQw=="
			crossorigin="anonymous"
			referrerpolicy="no-referrer"
		></script>
		<script src="https://unpkg.com/swiper@7/swiper-bundle.min.js"></script>
		<script src="javascripts/results.js"></script>
		<script src="javascripts/nav.js"></script>
		<script src="javascripts/chatbot.js"></script>
	</body>
</html>
