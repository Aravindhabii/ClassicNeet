            <span><img src="../images/aboutus/dot2.png" /></span>
          </div>
        </div> -->
				<div class="timelinePara">
					<% content.forEach(cont=> { %>
					<p><%= cont.content %> - <%= cont.year %></p>
					<% }) %>
				</div>
			</div>
		</section>
		<section class="galleryNew">
			<h1 class="galleryHead">Gallery</h1>
			<select
				class="form-select form-select-lg mb-3 dropdownNeet"
				aria-label=".form-select-lg example"
				data-select=""
			>
				<% folderArray.forEach((folder,i)=> { %>
				<option
					class="opt"
					value="<%= folder %>"
					selected="<%=i==0?true:false%>"
					data-year="<%= folder %>"
				>
					NEET <%= folder %>
				</option>
				<% }) %>
			</select>

			<div class="pagination">
				<section class="galleryMain tableList"></section>
				<div class="pagination-block">
					<span class="pageButton outline-none" id="button_prev"><</span>
					<span id="page_number" class="outline-none"></span>
					<span class="pageButton outline-none" id="button_next">></span>
				</div>
			</div>
		</section>

		<%- include('./layout/footer') %>
		<script
			src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"
			referrerpolicy="no-referrer"
		></script>
		<script src="javascripts/aboutus.js"></script>
		<script src="javascripts/nav.js"></script>
		<script src="javascripts/common.js"></script>
		<script src="javascripts/chatbot.js"></script>
	</body>
</html>
